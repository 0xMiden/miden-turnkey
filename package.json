{
  "name": "@miden-sdk/miden-turnkey",
  "private": true,
  "version": "1.0.0",
  "description": "Miden Turnkey Integration SDK",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "node --test tests",
    "build": "rm -rf dist && node scripts/build.mjs && tsc --emitDeclarationOnly --outDir dist/types",
    "publish": "node scripts/publish.mjs",
    "prepack": "yarn build",
    "postpack": "node scripts/postpack.mjs"
  },
  "keywords": [
    "miden",
    "turnkey",
    "wallet",
    "web3",
    "blockchain"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xPolygonMiden/miden-turnkey.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@1.22.22",
  "devDependencies": {
    "@types/react": "^19.0.0",
    "esbuild": "^0.24.0",
    "glob": "^11.0.0",
    "react": "^19.2.0",
    "react-test-renderer": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@demox-labs/miden-sdk": "^0.12.5",
    "@turnkey/core": "^1.8.2",
    "@turnkey/http": "^3.15.0",
    "@turnkey/sdk-browser": "^5.13.4"
  },
  "peerDependencies": {},
  "files": [
    "dist"
  ]
}
